@model Piranha.Models.PageModel
@using Catfish.Models.Regions;
@using Piranha.Extend.Regions;
@using Catfish.Helpers;

@section Head
{
    <script src="@Url.Content("~/Scripts/form.js")" type="text/javascript"></script>
}

@{
    ViewBag.Title = Model.Page.Title;

    foreach (KeyValuePair<string, object> region in Model.Regions)
    {
        @Html.DisplayFor(m => region.Value)
    }

}
