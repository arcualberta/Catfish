@model Catfish.Models.Blocks.CssBlock

@*

   NO NEED TO RENDER THIS BLOCK HERE SINCE WE RENDER IT AT THE HEAD SECTION IN THE LAYOUT.CSHTML FILE 
    
@{
    bool containStyleTag = string.IsNullOrEmpty(Model.CssVal.Value)? false : Model.CssVal.Value.Contains("<style>") ? true : false;
    
}

@if (containStyleTag)
{
    Html.Raw(Model.CssVal.Value);
}
else
{
    
    <style>
        @Html.Raw(Model.CssVal.Value)
    </style>
}*@