@model Catfish.Models.MediaPage
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    
    ViewBag.Title = "Social Media Page Template";
}
    <div class="main">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-sm-8">
                    @Html.DisplayFor(m => m.Blocks)
                </div>
                <div class="col-sm-4">
                    @if (Model.CalendarRegion != null)
                    {
                        if (!string.IsNullOrEmpty(Model.CalendarRegion.ApiKey.Value) && !string.IsNullOrEmpty(Model.CalendarRegion.CalendarId.Value))
                        {
                            <partial name="Partial/GoogleCalendarRegion.cshtml" model="@Model.CalendarRegion" />
                        }
                    }
                    @if (Model.FacebookTimeline != null)
                    {
                        if (!string.IsNullOrEmpty(Model.FacebookTimeline.FacebookUrl.Value))
                        {
                            <patial name="Partial/FacebookTimeline.cshtml" model="@Model.FacebookTimeline" />
                        }
                    }

                    @if (Model.TwitterRegion != null)
                    {
                        if (!string.IsNullOrEmpty(Model.TwitterRegion.TwitterUrl.Value))
                        {
                            <partial name="Partial/TwitterRegion.cshtml" model="@Model.TwitterRegion" />
                        }
                    }

                </div>
            </div>
        </div>
    </div>