@model Piranha.Entities.Post
@* Used to handle a list of posts. *@

@try
{
    <text>@Html.Partial("_CustomPost")</text>
}
catch (Exception)
{
    // custom view does not exist.
    <div class="post col-xs-12">
        <h3><a href="#">@Model.Title</a></h3>
        <div>@Model.Excerpt</div>
    </div>
}
