
@inject CatfishWebExtensions.Interfaces.IAssetRegistry _assetRegistry;

@{
    foreach (var asset in _assetRegistry.GetStylesheets())
    {
        <link rel="stylesheet" href="@asset" />
    }

    foreach (var asset in _assetRegistry.GetScripts())
    {
        <script src="@asset"></script>
    }

    foreach (var asset in _assetRegistry.GetModules())
    {
        <script type="module" src="@asset"></script>
    }

}