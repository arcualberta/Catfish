@model CatfishWebExtensions.Models.Blocks.GoogleCalendar

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@inject ICatfishAppConfiguration _catfishConfiguration;

@{
    var calIds = Model.getCalendarIds();
    var viewData = new ViewDataDictionary(this.ViewData)
    {
        { "data-api-key", _catfishConfiguration.GetGoogleCalendarAPIKey() },

        { "data-calendar-ids", Model.getCalendarIds() },
        { "data-css-class", Model.CssClass.Value },
        { "data-display-style", Model.getDisplayStyle() },
         { "data-calendar-title", Model.Title.Value },
          { "data-calendar-description", Model.Description.Value }
    };
}

<partial name="_AppletLoader.cshtml" view-data="@viewData" />

