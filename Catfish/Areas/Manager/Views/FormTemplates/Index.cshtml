@model IEnumerable<Catfish.Core.Models.Form>
@{ ViewBag.Title = "Forms"; }

@section Toolbar {
    @*@Html.Partial("Partial/IndexToolbar")*@

    @Html.Partial("Partial/Tabs")
    <div class="toolbar">
        <div class="inner">
            <ul>
                <li>
                    <a id="toolbar_add_button" href="@Url.Action("edit")" class="add">
                        @Piranha.Resources.Global.ToolbarAdd
                    </a>
                </li>
                <li>
                    <a id="toolbar_refresh_button" href="@Url.Action("index")" class="refresh">
                        @Piranha.Resources.Global.ToolbarReload
                    </a>
                </li>
                <li>
                    <a id="toolbar_ingest_button" href="@Url.Action("ingest")" class="ingest">
                        Ingest
                    </a>
                </li>
            </ul>
            @*<button class="search" title="@Piranha.Resources.Global.ToolbarSearch"></button>
                @Html.TextBox("search")*@
            <div class="clear"></div>
        </div>
