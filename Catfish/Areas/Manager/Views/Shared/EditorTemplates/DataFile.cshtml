@model Catfish.Core.Models.DataFile

@Html.Hidden("ModelType", Model.GetType().AssemblyQualifiedName, new Dictionary<string, object>()
{
    {"class", "model-type"}
})
@Html.HiddenFor(m => m.Id)
@Html.HiddenFor(m => m.Url)
@Html.HiddenFor(m => m.FileName)
@Html.HiddenFor(m => m.Thumbnail)

<img src="@Model.Thumbnail" width="40" height="30" alt="@Model.FileName" /> @Model.FileName


