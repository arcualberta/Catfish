@model Catfish.Core.Models.Aggregation
@using Catfish.Core.Models;

@{ IEnumerable<Aggregation> parents = Model.ParentMembers.Where(p => typeof(Collection).IsAssignableFrom(p.GetType())); }
<div class="bs entity-content">
    <div class="box">
        <div class="title">
            Parent Collections
        </div>
        @Html.Partial("Partial/ObjectList", parents, new ViewDataDictionary() { { "title", "Parent Entities" }, { "controller", "collections" } })
    </div>
</div>
