@model Catfish.Areas.Manager.Models.ViewModels.EntityAccessDefinitionsViewModel
@using Catfish.Core.Models.Access
@{

    ViewBag.Title = "Edit Access Definition";
    string subTitle = Model.EntityName + " Access Permissions";

}

@section HEAD
{
    <script src="~/Scripts/jquery-ui.min.js" type="text/javascript"></script>

    <link type="text/css" rel="stylesheet" href="~/content/jquery-ui.min.css" />
}

@section Toolbar {
    @Html.Partial("Partial/EditToolbar", Model, new ViewDataDictionary() { { "id", Model.Id } })
}

@Html.Partial("Partial/EntityAccessGroup", Model, new ViewDataDictionary(this.ViewData) { { "Controller", "Collections" }, { "Action", "AccessGroup" } })

