@model Catfish.Core.Models.Data.FormSubmission
@using Catfish.Core.Models;

<div class="inner">
    <div class="box">
        @{ 
            string formTitle = string.Join(" | ", Model.FormData.GetNames(false).Where(n => !string.IsNullOrEmpty(n.Value)).Select(n => n.Value));
            if (string.IsNullOrEmpty(formTitle))
            {
                formTitle = "Form Data";
            }
        }
        <div class="title">
            @formTitle
        </div>
        <div class="inner">
            @Html.DisplayFor(m => m.FormData)
        </div>
    </div>
</div>
