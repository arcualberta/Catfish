@model Catfish.Core.Models.Contents.Fields.InfoSection

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<td>
    @Model.GetType().Name
</td>

<td colspan="3">
    @foreach(var txt in Model.Content.Values)
    {
        <div id="@txt.Id">
            @Html.Raw(txt.Value)
        </div>
    }
</td>