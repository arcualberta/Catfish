@model Catfish.Areas.Applets.Models.Blocks.ChildFormSubmission
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
	var viewData = new ViewDataDictionary(this.ViewData)
{
		{ "data-template-id", Model.SelectedItemTemplate.Value },
		{ "data-child-form-id", Model.SelectedChildForm.Value },
		{ "data-display-child-list", Model.DisplayChildList.Value },
		{ "data-response-form-id", Model.DisplayChildList.Value ? Model.SelectedResponseForm.Value : "" },
		{ "data-is-admin", User.IsInRole("SysAdmin") },

	};
}
<partial name="Shared/_AppletLoader" view-data="@viewData" />

