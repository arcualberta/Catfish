@model Catfish.Areas.Applets.Models.Blocks.KeywordSearch
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@inject Catfish.Areas.Applets.Services.IAssetRegistry _assetRegistry;

@{
   
    var viewData = new ViewDataDictionary(this.ViewData)
{
      
        { "data-collection-id", Model.SelectedCollection?.Value },
        { "data-template-id", Model.SelectedItemTemplate?.Value },
        { "data-keyword-metadata-set-id", Model.ClassificationMetadataSetId?.Value },
        { "data-keyword-source-id", Model.KeywordSourceId?.Value },
        { "data-block-title", Model.BlockTitle?.Value },
        { "data-block-description", Model.Description?.Value },
        { "data-enable-freetext-search", Model.EnabledFreeTextSearch?.Value},
        { "data-display-format", Model.GetSelectedDisplayFormat()}
    };

    //Registering assets
    _assetRegistry.RegisterStylesheet("/assets/dist/applets/styles/KeywordSearch.css", "/assets/dist/applets/styles/KeywordSearch.css");

}
<partial name="Shared/_AppletLoader" view-data="@viewData" />

