@page "~/new"
@model Catfish.Pages.StartPageModel

@{
    ViewData["Title"] = "Start Page";
}

<h1>Start Page</h1>

<div class="container">
    @foreach (var template in Model.ItemTemplates)
    {
        var url = "/entity/create/" + template.Id;
        <div>
            <a href="@url">@template.TemplateName</a>
        </div>
    }
</div>

