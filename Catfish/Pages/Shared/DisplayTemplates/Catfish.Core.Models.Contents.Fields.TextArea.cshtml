@model Catfish.Core.Models.Contents.Fields.TextArea

<div class="control">@Html.DisplayFor(model => model.Name, Model.Name.GetType().ToString())</div>
<br />
@for (int i = 0; i < Model.Values.Count; ++i)
    @Html.DisplayFor(model => Model.Values[i], Model.Values[i].GetType().ToString(), null, new { multiline = true })
